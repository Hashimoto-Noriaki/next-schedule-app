{
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "error",
    "@next/next/no-img-element": "off",
    "prettier/prettier": ["error"]
  },
  "ignorePatterns": [
    "src/components/digital-agency/**/*",
    "package.json",
    "package-lock.json",
    "tsconfig.json",
    ".eslintrc.json",
    "src/components/HTMLComponentWithScript.jsx"
  ]
}
